    "react-swipeable-views": "^0.13.9",
    "react-swipeable-views-utils": "^0.13.9",
    "react-text-loop": "^2.0.1",
    "rebass": "^4.0.7",
    "redux": "^4.0.5",
    "styled-components": "^5.1.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@percy/cypress": "^2.1.1",
    "axe-core": "^3.5.3",
    "chalk": "^4.0.0",
    "contentful-import": "^7.7.7",
    "cypress": "^4.3.0",
    "cypress-axe": "^0.8.1",
    "eslint": "^5.6.0",
